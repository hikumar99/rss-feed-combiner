import requests
import csv

def get_feeds_from_google_sheet(csv_url):
    resp = requests.get(csv_url)
    lines = resp.text.splitlines()
    reader = csv.reader(lines)
    feeds = [row[0] for row in reader if row]  # assume URLs in first column
    return feeds

# Usage:
FEED_CSV_URL = "https://docs.google.com/spreadsheets/d/e/2PACX-1vQOJhsHTO7hWYd15XnQpGvscmeCqYa3NPT6yt6nai0gRs8ocbuTZsuh_FxxcJqIBS9_RDsSNM5fbLVp/pub?gid=1623404092&single=true&output=csv"
feeds = get_feeds_from_google_sheet(FEED_CSV_URL)
